{
  "env": {
    "development": {
      "presets": ["next/babel"],
      "plugins": [
        ["babel-plugin-styled-components", { "ssr": true, "displayName": true }]
      ]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [
        [
          "babel-plugin-styled-components",
          { "ssr": true, "displayName": false }
        ]
      ]
    }
  }
}
